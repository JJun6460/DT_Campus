import random

Ans = random.randint(1, 100)
print(f'정답 : {Ans}')

s = 0
count = 0

while s != Ans:
    
    s = int(input('숫자를 입력하세요 : '))
    count += 1

    if count < 5:

        if s > Ans:
            print('DOWN!')

        elif s < Ans:
            print('UP!')

        else:
            print(f'"정답입니다!"\n"{count}번만에 맞췄습니다."')
            break

    else:
        print(f'실패했습니다. 정답은 {Ans}였습니다.')
        break
